 <%= form_for(:session, url: login_path) do |f| %>
    <div class="form-group m-2">
      <%= f.label :username %>
      <%= f.text_field :username, class: "form-control" %>
    </div>
    <div class="actions m-2">
      <%= f.submit :Submit, value: "Login", class: "btn btn-success" %>
      </div>
    <% end %>
    <p>Create new account <%= link_to "Sign up", new_user_path %></p>
    
